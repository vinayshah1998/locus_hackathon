{
  "name": "my-locus-app",
  "version": "1.0.0",
  "description": "A Locus-powered application using Claude SDK and TypeScript",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "start": "tsx index.ts",
    "dev": "tsx watch index.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "a2a": "tsx src/main.ts",
    "a2a:vinay": "AGENT_PORT=4000 AGENT_IDENTITY=vinay AGENT_PERSONALITY=balanced tsx src/main.ts",
    "a2a:dhruv": "AGENT_PORT=4001 AGENT_IDENTITY=dhruv AGENT_PERSONALITY=conservative tsx src/main.ts"
  },
  "keywords": [
    "locus",
    "claude",
    "anthropic",
    "mcp",
    "ai",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.42",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "uuid": "^13.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^22.10.2",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
